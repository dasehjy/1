    "import random\n",
    "import math\n",
    "def estimate(times):\n",
    "    sum=0\n",
    "    for i in range(times):\n",
    "        x=random.uniform(2,3)\n",
    "        y=random.uniform(0,100)\n",
    "        d=x*x+4*x*math.sin(x)-y\n",
    "        if d>0:\n",
    "            sum+=1\n",
    "    print((sum*100)/times)\n",
    "estimate(1000000)"
   ]
